mixin header()  
  header.header
    .header__top
      .container-wrapper
        .row.end.center 
          .col__right.row.center
            .header__top__col.header__top__city
              span Ваш город: 
              select()
                option(value="Москва1") Москва1
                option(value="Москва3") Москва3
                option(value="Москва2") Москва2
            .header__top__col.header__top__opend
              span Ежедневно с 
              strong 9 
              | до 
              strong 21
            .header__top__col.header__top__phone
              span.header__top__icon 8 495 243 41 00
            .header__top__col.header__top__phone 
              span.header__top__circle 8 800 505 59 76
    .header__middle 
      .container-wrapper
        .row.between
          .col__left 
            a(href="index.html").header__middle__logo
          .col__right.row.center
            .header__middle__col.header__middle__search
              input(type="text", placeholder='Поиск').header__middle__search-input
            .header__middle__col.header__middle__favorite
              a(href="").button.button_grey.button_favorite Избранное
            .header__middle__col.header__middle__compare
              a(href="").button.button_grey.button_compare Сравнение
                span.header__middle__counter 3
            .header__middle__col.header__middle__cart 
              a(href="").button.button_yellow.button_cart Корзина
                span.header__middle__counter 3
