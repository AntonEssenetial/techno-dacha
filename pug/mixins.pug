//- Navigation multi-level
mixin main-menu(data)
  nav.main-menu&attributes(attributes)
    ul.row.between.center
      each item, i in data
        if i === pageId
          li.main-menu__item.main-menu__item_active
            a(href=item.url).main-menu__link= item.title
            if (item.submenu !== false)
              +main-menu-submenu(item.submenu)
        else
          li.main-menu__item
            a(href=item.url).main-menu__link= item.title
            if (item.submenu !== false)
              +main-menu-submenu(item.submenu)
mixin main-menu-submenu(data)
  ul.main-menu__sub-menu.animated.fadeIn&attributes(attributes)
    .main-menu__sub-menu-wrapper
      each item, i in data
        if i === subId
          li.main-menu__sub-menu__item.main-menu__sub-menu__item_active
            a(href=item.url).main-menu__sub-menu__link
              span.main-menu__sub-menu__img-wrapper
                img(src=item.src , alt="")
              = item.title
        else  
          li.main-menu__sub-menu__item
            a(href=item.url).main-menu__sub-menu__link
              span.main-menu__sub-menu__img-wrapper
                img(src=item.src , alt="")
              = item.title


//- Navigation menu
mixin nav(items)
  nav.main-menu
    ul.menu
      each item, i in items
        if i === pageId
          li.menu__item.menu__item_active
            a.menu__link(href=item.href)= item.name
        else
          li.menu__item
            a.menu__link(href=item.href)= item.name


//- Sandwitch
mixin sandwitch()
  button.toggle-menu.jsTag
    .sandwitch
      span.sw-topper
      span.sw-bottom
      span.sw-footer


//- Bread
mixin bread-crumbs(data)
  .bread-crumbs
    ul
      each ent, num in data
        if (num+1 < data.length)
          li.bread-crumbs__item 
            a(href="#").bread-crumbs__link= ent
            .bread-crumbs__separator
        else
          li.bread-crumbs__item= ent


//- Heading
mixin heading(item)
  if item[0]= item[0]
    h2.section__content-h2= item[0]
  if item[1]= item[1]
    .section__content-text
      p= item[1]
      if item[2]= item[2]
        p= item[2] 
          if item[3]= item[3]
            a(href='mailto:')= item[3]


